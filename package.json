{
  "name": "@flockler/react-flockler-embed",
  "version": "1.0.4",
  "description": "React component for rendering a Flockler Embed",
  "files": [
    "dist"
  ],
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "scripts": {
    "build": "rm -rf ./dist/* && npm run build:esm && npm run build:cjs",
    "build:esm": "tsc",
    "build:cjs": "tsc --module commonjs --outDir dist/cjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/flockler/react-flockler-embed.git"
  },
  "keywords": [
    "flockler",
    "embed",
    "social wall",
    "social feed"
  ],
  "author": "Flockler Oy <team@flockler.com> (https://flockler.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/flockler/react-flockler-embed/issues"
  },
  "homepage": "https://github.com/flockler/react-flockler-embed#readme",
  "peerDependencies": {
    "react": "^16.8.0 || ^17.0.0 || ^18.0.0",
    "react-dom": "^16.8.0 || ^17.0.0 || ^18.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.35",
    "prettier": "^2.8.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^4.4.3"
  }
}
